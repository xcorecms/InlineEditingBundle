{
  "name": "xcore/inline-editing-bundle",
  "type": "symfony-bundle",
  "description": "Content inline editable bundle for Symfony.",
  "keywords": [
    "symfony",
    "xcore"
  ],
  "homepage": "https://appio.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/orm": "^2.5",
    "xcore/inline-editing": "^1.3",
    "symfony/framework-bundle": "^5.0",
    "symfony/expression-language": "^5.0",
    "symfony/validator": "^5.0",
    "symfony/property-access": "^5.0",
    "twig/twig": "^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan": "^1.0"
  },
  "suggest": {
    "symfony/security": "If you wanna use default permission checker subscriber"
  },
  "autoload": {
    "psr-4": {
      "XcoreCMS\\InlineEditingBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XcoreCMS\\InlineEditingBundle\\Tests\\": "tests/"
    }
  }
}
